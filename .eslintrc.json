{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-shadow": "off",
    "no-param-reassign": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-react": "off",
    "eol-last": "off",
    "import/extensions": [ 1, {
      "js": "", "json": "always"
    }]
  },
  "ignorePatterns": [
    "dist/",
    "build/"
  ]
}
